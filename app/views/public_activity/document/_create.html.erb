<div class="listactivities">
<% if activity.trackable %>
	<%= link_to activity.owner.email, user_profile_path(activity.owner) %>
	has uploaded new  
	<%= link_to "document", batch_document_path(activity.trackable.batch, activity.trackable) %>
	<p><%= link_to activity.trackable.title, batch_document_path(activity.trackable.batch, activity.trackable) %></p>
<% else %>
  which has since been removed
<% end %>
</div>
